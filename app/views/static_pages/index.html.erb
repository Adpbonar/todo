<section class="todoapp">
  <header class="header">
    <h1>todo list</h1>
    <form id="new-form">
      <input type="text" class="new-todo" placeholder="Add a chore to the list" />
    </form>
  </header>
    <section class="main">
      <ul class="todo-list">
        <li>
          <div class="view">
            <input class="toggle" type="checkbox">
            <label></label>
          </div>
        </div>
      <li>
    </ul>
  </section>
</section>

